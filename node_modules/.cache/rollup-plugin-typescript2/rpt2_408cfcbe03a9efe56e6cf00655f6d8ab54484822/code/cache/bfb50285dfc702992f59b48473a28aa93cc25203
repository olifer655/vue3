{"code":"import { createVNode } from \"./createVNode\";\r\nexport function createAppAPI(render) {\r\n    return (rootComponent, rootProps) => {\r\n        let isMounted = false;\r\n        const app = {\r\n            mount(container) {\r\n                // 1.创造组件虚拟节点 \r\n                let vnode = createVNode(rootComponent, rootProps); // h函数\r\n                // 2.挂载的核心就是根据传入的组件对象 创造一个组件的虚拟节点 ，在将这个虚拟节点渲染到容器中\r\n                render(vnode, container);\r\n                if (!isMounted) {\r\n                    isMounted = true;\r\n                }\r\n            }\r\n        };\r\n        return app;\r\n    };\r\n}\r\n//# sourceMappingURL=apiCreateApp.js.map","references":["C:/Users/test1/Desktop/vue-topic/vue-train-5/packages/runtime-core/src/createVNode.ts"],"map":"{\"version\":3,\"file\":\"apiCreateApp.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../../../../packages/runtime-core/src/apiCreateApp.ts\"],\"names\":[],\"mappings\":\"AAAA,OAAO,EAAE,WAAW,EAAE,MAAM,eAAe,CAAC;AAE5C,MAAM,UAAU,YAAY,CAAC,MAAM;IAC/B,OAAO,CAAC,aAAa,EAAE,SAAS,EAAE,EAAE;QAChC,IAAI,SAAS,GAAG,KAAK,CAAC;QACtB,MAAM,GAAG,GAAG;YACR,KAAK,CAAC,SAAS;gBACX,cAAc;gBACd,IAAI,KAAK,GAAG,WAAW,CAAC,aAAa,EAAE,SAAS,CAAC,CAAC,CAAC,MAAM;gBACzD,iDAAiD;gBACjD,MAAM,CAAC,KAAK,EAAE,SAAS,CAAC,CAAA;gBACxB,IAAI,CAAC,SAAS,EAAE;oBACZ,SAAS,GAAG,IAAI,CAAC;iBACpB;YACL,CAAC;SACJ,CAAA;QACD,OAAO,GAAG,CAAA;IACd,CAAC,CAAA;AACL,CAAC\"}"}
