#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/test1/Desktop/vue-topic/vue-train-5/node_modules/.pnpm/typescript@4.5.2/node_modules/typescript/bin/node_modules:/mnt/c/Users/test1/Desktop/vue-topic/vue-train-5/node_modules/.pnpm/typescript@4.5.2/node_modules/typescript/node_modules:/mnt/c/Users/test1/Desktop/vue-topic/vue-train-5/node_modules/.pnpm/typescript@4.5.2/node_modules:/mnt/c/Users/test1/Desktop/vue-topic/vue-train-5/node_modules/.pnpm/node_modules:/mnt/c/Users/test1/Desktop/vue-topic/vue-train-5/node_modules:/mnt/c/Users/test1/Desktop/vue-topic/node_modules:/mnt/c/Users/test1/Desktop/node_modules:/mnt/c/Users/test1/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/test1/Desktop/vue-topic/vue-train-5/node_modules/.pnpm/rollup-plugin-typescript2@0.31.0_rollup@2.60.0+typescript@4.5.2/node_modules/rollup-plugin-typescript2/node_modules:/mnt/c/Users/test1/Desktop/vue-topic/vue-train-5/node_modules/.pnpm/rollup-plugin-typescript2@0.31.0_rollup@2.60.0+typescript@4.5.2/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/test1/Desktop/vue-topic/vue-train-5/node_modules/.pnpm/typescript@4.5.2/node_modules/typescript/bin/node_modules:/mnt/c/Users/test1/Desktop/vue-topic/vue-train-5/node_modules/.pnpm/typescript@4.5.2/node_modules/typescript/node_modules:/mnt/c/Users/test1/Desktop/vue-topic/vue-train-5/node_modules/.pnpm/typescript@4.5.2/node_modules:/mnt/c/Users/test1/Desktop/vue-topic/vue-train-5/node_modules/.pnpm/node_modules:/mnt/c/Users/test1/Desktop/vue-topic/vue-train-5/node_modules:/mnt/c/Users/test1/Desktop/vue-topic/node_modules:/mnt/c/Users/test1/Desktop/node_modules:/mnt/c/Users/test1/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/test1/Desktop/vue-topic/vue-train-5/node_modules/.pnpm/rollup-plugin-typescript2@0.31.0_rollup@2.60.0+typescript@4.5.2/node_modules/rollup-plugin-typescript2/node_modules:/mnt/c/Users/test1/Desktop/vue-topic/vue-train-5/node_modules/.pnpm/rollup-plugin-typescript2@0.31.0_rollup@2.60.0+typescript@4.5.2/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../typescript@4.5.2/node_modules/typescript/bin/tsserver" "$@"
else
  exec node  "$basedir/../../../../../typescript@4.5.2/node_modules/typescript/bin/tsserver" "$@"
fi
